<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
	<system.diagnostics>
		<trace autoflush="true">
			<listeners>
				<add name="DateFolderMakerLog" type="System.Diagnostics.TextWriterTraceListener" initializeData="Logs.txt" />
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>
